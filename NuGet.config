<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <config>
    <!-- Ensure packages are restored into container-local folder -->
    <add key="globalPackagesFolder" value="/root/.nuget/packages" />
  </config>
  <!-- Clear any fallback package folders that might reference Windows paths -->
  <fallbackPackageFolders>
  </fallbackPackageFolders>
</configuration>
